{
  "name": "site",
  "private": true,
  "description": "Personal Gatsby blog with interactive background system",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@tailwindcss/typography": "^0.5.15",
    "assert": "^2.1.0",
    "browserify-zlib": "^0.2.0",
    "class-variance-authority": "^0.7.0",
    "constants-browserify": "^1.0.0",
    "d3-drag": "^3.0.0",
    "d3-force": "^3.0.0",
    "d3-selection": "^3.0.0",
    "d3-zoom": "^3.0.0",
    "dotenv": "^16.4.7",
    "gatsby": "^5.13.7",
    "gatsby-plugin-image": "^3.13.1",
    "gatsby-plugin-manifest": "^5.13.1",
    "gatsby-plugin-purgecss": "^6.2.1",
    "gatsby-plugin-react-helmet": "^6.13.1",
    "gatsby-plugin-sharp": "^5.13.1",
    "gatsby-plugin-typescript": "^5.14.0",
    "gatsby-remark-katex": "^7.14.0",
    "gatsby-remark-prismjs": "^7.14.0",
    "gatsby-source-filesystem": "^5.14.0",
    "gatsby-transformer-remark": "^6.14.0",
    "gatsby-transformer-sharp": "^5.13.1",
    "katex": "^0.13.24",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "prismjs": "^1.30.0",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "rehype-katex": "^7.0.1",
    "remark-math": "^6.0.0",
    "sharp": "^0.33.5",
    "slugify": "^1.6.6",
    "stream-browserify": "^3.0.0",
    "tailwind": "^4.0.0",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "url": "^0.11.4",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@playwright/test": "^1.49.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/d3-drag": "^3.0.7",
    "@types/d3-force": "^3.0.10",
    "@types/d3-selection": "^3.0.11",
    "@types/d3-zoom": "^3.0.8",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "autoprefixer": "^10.4.20",
    "babel-jest": "^29.7.0",
    "babel-plugin-import": "^1.13.8",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "babel-preset-gatsby": "^3.14.0",
    "bundlesize": "^0.18.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "gatsby-plugin-postcss": "^6.13.1",
    "gh-pages": "^6.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "jest-webgl-canvas-mock": "^2.5.2",
    "json-schema": "^0.4.0",
    "lighthouse": "^12.2.1",
    "null-loader": "^4.0.1",
    "postcss": "^8.5.1",
    "prettier": "^2.8.8",
    "puppeteer": "^23.10.4",
    "react-test-renderer": "^18.3.1",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.3",
    "webpack-bundle-analyzer": "^4.10.2",
    "wrangler": "^4.28.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "0BSD",
  "scripts": {
    "build": "node scripts/generate-headers.js && gatsby build",
    "build:gh": "node scripts/generate-headers.js && DEPLOY_TARGET=github gatsby build --prefix-paths",
    "build:cf": "node scripts/generate-headers.js && gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,css}\"",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy:cf": "npm run build:cf && npx wrangler pages deploy public --project-name rynsy-personal-site",
    "deploy:cf:preview": "npm run build:cf && npx wrangler pages deploy public --project-name rynsy-personal-site --compatibility-date 2024-10-01",
    "deploy:cf:integrated": "node scripts/deploy-integrated.js deploy:cf",
    "deploy:cf:preview:integrated": "node scripts/deploy-integrated.js deploy:cf:preview",
    "deploy:gh:integrated": "node scripts/deploy-integrated.js deploy:gh",
    "build:integrated": "node scripts/deploy-integrated.js build",
    "new-blog": "node scripts/new-blog.js",
    "generate-headers": "node scripts/generate-headers.js",
    "update-csp": "node scripts/generate-headers.js",
    "add-csp-domain": "node scripts/add-csp-domain.js",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "lint:check": "eslint src --ext .ts,.tsx",
    "analyze-bundle": "ANALYZE_BUNDLE=true npm run build",
    "dev:type-safe": "npm run type-check && npm run develop",
    "build:type-safe": "npm run type-check && npm run build",
    "analyze:full": "node scripts/type-check-and-analyze.js",
    "precommit": "npm run lint:check && npm run type-check",
    "ci:test": "npm run analyze:full",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --passWithNoTests",
    "test:unit": "jest --testPathPattern=__tests__/unit",
    "test:integration": "jest --testPathPattern=__tests__/integration",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:docker": "DOCKER_TEST=true playwright test --config=playwright.config.ts",
    "test:production-errors": "playwright test tests/e2e/console-error-monitoring.spec.ts tests/e2e/network-error-monitoring.spec.ts tests/e2e/react-error-monitoring.spec.ts",
    "test:production-health": "playwright test tests/e2e/production-health-monitoring.spec.ts",
    "test:console-errors": "playwright test tests/e2e/console-error-monitoring.spec.ts",
    "test:network-errors": "playwright test tests/e2e/network-error-monitoring.spec.ts", 
    "test:react-errors": "playwright test tests/e2e/react-error-monitoring.spec.ts",
    "test:docker-monitoring": "DOCKER_TEST=true playwright test tests/e2e/docker-production-monitoring.spec.ts",
    "test:production-gate": "DOCKER_TEST=true playwright test tests/e2e/production-health-monitoring.spec.ts tests/e2e/docker-production-monitoring.spec.ts --grep='should pass production readiness gate|should handle production deployment gate'",
    "test:cloudflare-errors": "playwright test tests/e2e/console-error-monitoring.spec.ts --grep='Cloudflare'",
    "test:gatsby-errors": "playwright test tests/e2e/console-error-monitoring.spec.ts --grep='Gatsby'",
    "test:react423-errors": "playwright test tests/e2e/react-error-monitoring.spec.ts --grep='#423'",
    "test:accessibility": "playwright test --grep=\"@accessibility\"",
    "test:performance": "playwright test --grep=\"@performance\"",
    "test:mobile": "playwright test --project=Mobile",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:all-monitoring": "npm run test:production-errors && npm run test:production-health",
    "test:bundle-size": "bundlesize",
    "test:lighthouse": "node scripts/lighthouse-ci.js",
    "playwright:install": "playwright install --with-deps",
    "performance:benchmark": "node scripts/performance-benchmark.js",
    "accessibility:audit": "node scripts/accessibility-audit.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rynsy/gatsby-demo"
  },
  "bugs": {
    "url": "https://github.com/rynsy/gatsby-demo/issues"
  },
  "bundlesize": [
    {
      "path": "./public/**/*.js",
      "maxSize": "200 kB",
      "compression": "gzip"
    },
    {
      "path": "./public/**/*.css",
      "maxSize": "50 kB",
      "compression": "gzip"
    }
  ]
}