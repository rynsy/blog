{
  "name": "site",
  "private": true,
  "description": "Personal Gatsby blog with interactive background system",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@tailwindcss/typography": "^0.5.15",
    "assert": "^2.1.0",
    "browserify-zlib": "^0.2.0",
    "class-variance-authority": "^0.7.0",
    "constants-browserify": "^1.0.0",
    "d3-drag": "^3.0.0",
    "d3-force": "^3.0.0",
    "d3-selection": "^3.0.0",
    "d3-zoom": "^3.0.0",
    "dotenv": "^16.4.7",
    "gatsby": "^5.13.7",
    "gatsby-plugin-image": "^3.13.1",
    "gatsby-plugin-manifest": "^5.13.1",
    "gatsby-plugin-purgecss": "^6.2.1",
    "gatsby-plugin-react-helmet": "^6.13.1",
    "gatsby-plugin-sharp": "^5.13.1",
    "gatsby-plugin-typescript": "^5.14.0",
    "gatsby-remark-katex": "^7.14.0",
    "gatsby-remark-prismjs": "^7.14.0",
    "gatsby-source-filesystem": "^5.14.0",
    "gatsby-transformer-remark": "^6.14.0",
    "gatsby-transformer-sharp": "^5.13.1",
    "katex": "^0.13.24",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "prismjs": "^1.30.0",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "rehype-katex": "^7.0.1",
    "remark-math": "^6.0.0",
    "sharp": "^0.33.5",
    "slugify": "^1.6.6",
    "stream-browserify": "^3.0.0",
    "tailwind": "^4.0.0",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "url": "^0.11.4",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@types/d3-drag": "^3.0.7",
    "@types/d3-force": "^3.0.10",
    "@types/d3-selection": "^3.0.11",
    "@types/d3-zoom": "^3.0.8",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "gatsby-plugin-postcss": "^6.13.1",
    "gh-pages": "^6.2.0",
    "postcss": "^8.5.1",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.6.3",
    "wrangler": "^4.28.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "0BSD",
  "scripts": {
    "build": "node scripts/generate-headers.js && gatsby build",
    "build:gh": "node scripts/generate-headers.js && DEPLOY_TARGET=github gatsby build --prefix-paths",
    "build:cf": "node scripts/generate-headers.js && gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,css}\"",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy:cf": "npm run build:cf && npx wrangler pages deploy public --project-name rynsy-personal-site",
    "deploy:cf:preview": "npm run build:cf && npx wrangler pages deploy public --project-name rynsy-personal-site --compatibility-date 2024-10-01",
    "deploy:cf:integrated": "node scripts/deploy-integrated.js deploy:cf",
    "deploy:cf:preview:integrated": "node scripts/deploy-integrated.js deploy:cf:preview",
    "deploy:gh:integrated": "node scripts/deploy-integrated.js deploy:gh",
    "build:integrated": "node scripts/deploy-integrated.js build",
    "new-blog": "node scripts/new-blog.js",
    "generate-headers": "node scripts/generate-headers.js",
    "update-csp": "node scripts/generate-headers.js",
    "add-csp-domain": "node scripts/add-csp-domain.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rynsy/gatsby-demo"
  },
  "bugs": {
    "url": "https://github.com/rynsy/gatsby-demo/issues"
  },
  "bundlesize": [
    {
      "path": "./public/**/*.js",
      "maxSize": "200 kB",
      "compression": "gzip"
    },
    {
      "path": "./public/**/*.css",
      "maxSize": "50 kB",
      "compression": "gzip"
    }
  ]
}
